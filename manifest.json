{
  "manifest_version": 3,
  "name": "GitHub Name Changer",
  "description": "show nickname instead of id on GitHub",
  "version": "0.1.0",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["namechanger.js"]
    }
  ],
  "icons": {
    "16": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
